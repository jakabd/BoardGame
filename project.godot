; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Board Game"
config/description="TODO"
run/main_scene="res://Scenes/Board.tscn"
config/features=PackedStringArray("4.2", "Mobile")
config/icon="res://icon.svg"

[autoload]

Global="*res://Scripts/Global.gd"

[display]

window/size/viewport_width=1920
window/size/viewport_height=1080
window/size/mode=2
window/size/initial_position_type=3
window/size/borderless=true
window/stretch/mode="canvas_items"

[editor]

version_control/plugin_name="GitPlugin"
version_control/autoload_on_startup=true

[layer_names]

2d_physics/layer_1="Cards"
2d_physics/layer_2="Markers"
2d_physics/layer_3="Dice"

[physics]

common/enable_physics_object_picking_top_only=true

[rendering]

renderer/rendering_method="mobile"
